/btnDesign';\r\n@use 'abstracts/logoDesign';\r\n\r\nbody {\r\n  font: {\r\n    size: 1.2rem;\r\n    family: Helvetica, sans-serif;\r\n  }\r\n  background-color: c.$primary-color;\r\n}\r\n\r\n@mixin display($display, $align, $justify) {\r\n  display: $display;\r\n  align-items: $align;\r\n  justify-content: $justify;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\n@mixin image($width, $height, $bg-image) {\r\n  width: $width;\r\n  height: $height;\r\n  background-image: $bg-image;\r\n  background-repeat: no-repeat;\r\n  background-position: center;\r\n}\r\n\r\n@mixin colorSetup($color) {\r\n  @if $color == big {\r\n    background-color: c.$secondaryColor;\r\n  } @else {\r\n    background-color:  c.$tertiaryColor;\r\n  }\r\n }\r\n \r\n\r\n.container {\r\n  width: 100%;\r\n  height: 700px;\r\n  padding: 3rem;\r\n  @include colorSetup(big);\r\n  nav {\r\n    @include display(flex, center, space-between);\r\n    ul {\r\n      @include display(flex, center, center);\r\n      li {\r\n        list-style-type: none;\r\n        padding: 0.1rem  0.7rem;\r\n\r\n        a {\r\n          color: #2c3e50;\r\n        }\r\n\r\n        a:hover {\r\n          color: c.$headerColor;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .mainContainer {\r\n    @include display(flex, center, space-between);\r\n    margin-top: 3rem;\r\n    .image {\r\n      @include image(100%, 400px, url('abstracts/bigImage.png'));\r\n    }\r\n    .headingPara {\r\n      width: 50%;\r\n      padding: 0 1rem;\r\n      p {\r\n        margin: 0.8rem 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width:800px){\r\n  .container {\r\n    @include colorSetup(small);\r\n    nav {\r\n      ul {\r\n        li {\r\n          a {\r\n            color: c.$headerColor;\r\n          }\r\n          a:hover {\r\n            color: c.$logoColor;\r\n          }\r\n        }\r\n      }\r\n      \r\n    }\r\n    \r\n    .mainContainer {\r\n      @include display(block, center, center);\r\n      margin-top: 1rem;\r\n      .image {\r\n      @include image(318px, 159px, url('abstracts/smallImage.png'));\r\n      margin: 0 auto;\r\n      }\r\n      .headingPara {\r\n        width: 100%;\r\n        padding: 0;\r\n        margin: 1rem 0;\r\n        h2, p {\r\n          color: c.$headerColor;\r\n        }\r\n        p {\r\n          margin: 0.8rem 0;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"d554d459d60b4171e2f1b164a800a832","cacheData":{"env":{}}}